  "repository": {
    "type": "git",
    "url": "git://github.com/ahmadassaf/code-notes.git"
  },
  "main": "./lib/notes",
  "engines": {
    "node": ">= v0.4.7"
  },
  "contributors": [
    {
      "name": "Ahmad Assaf",
      "email": "me@ahmadassaf.com"
    }
  ],
  "dependencies": {
    "byline": "5.0.0",
    "chalk": "1.1.3",
